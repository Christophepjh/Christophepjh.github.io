{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
      "text": "Index of Services UK",
      "subtitle": [
        "Human health services TOTAL (Â£m)"
        
      ],
      "subtitleFontStyle": "italic",
      "subtitleFontSize": 10,
      "anchor": "start",
      "color": "black"
    },
    "width": 175,
    "height": 200,
    "data": {
      
      "url": "https://raw.githubusercontent.com/Christophepjh/Christophepjh.github.io/main/charts/ONSSectors/data_ONS-JR3D.json",
      "format": {"type": "json", "property": "years"}
    },
    "mark": {"type": "line", "color": "#F77F00", "strokeWidth": 2, "opacity": 1},
    "transform": [
      {"filter": "datum.year>minYear"},
      {"filter": "datum.year<maxYear"}
    ],
    "params": [
      {
        "name": "minYear",
        "value": 1996,
        "bind": {
          "input": "range",
          "min": 1997,
          "max": 2022,
          "step": 1,
          "name": "Start year:"
        }
      },
      {
        "name": "maxYear",
        "value": 2022,
        "bind": {
          "input": "range",
          "min": 1997,
          "max": 2022,
          "step": 1,
          "name": "End year:"
        }
      }
    ],
    "encoding": {
      "x": {"field": "date", "type": "temporal", "title": null, "axis": {"grid": false}},
      "y": {
        "field": "value",
        "type": "quantitative",
        "title": "Value (Â£m)",
        "axis": {"grid": false}
      }
    }
  }